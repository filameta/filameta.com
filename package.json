{
    "name": "filameta.com",
    "private": true,
    "license": "MIT",
    "repository": "git@github.com:filameta/filameta.com.git",
    "author": "Mark Lopez <m@silvenga.com>",
    "packageManager": "yarn@4.5.0",
    "type": "module",
    "browserslist": "defaults and fully supports es6-module, not op_mini all",
    "importSort": {
        ".ts,.tsx": {
            "style": "import-sort-style-module-compact",
            "options": {}
        }
    },
    "targets": {
        "default": {
            "distDir": "./dist"
        }
    },
    "@parcel/resolver-default": {
        "packageExports": true
    },
    "staticFiles": [
        {
            "staticPath": "src/static/",
            "staticOutPath": ""
        }
    ],
    "scripts": {
        "parcel": "parcel",
        "parcel:start": "parcel --port 9999 --config .parcelrc ./src/index.html",
        "parcel:build": "parcel build --config .prod.parcelrc ./src/index.html",
        "sri-dist": "posthtml -u posthtml-sri --posthtml-sri.basePath=dist/ dist/**/*.html",
        "lint": "eslint --max-warnings=0 src",
        "start": "yarn parcel:start",
        "clean": "rimraf ./dist && rimraf ./.parcel-cache",
        "build": "yarn clean && yarn parcel:build && yarn sri-dist",
        "shadcn": "shadcn"
    },
    "devDependencies": {
        "@eslint/js": "^9.12.0",
        "@parcel/optimizer-data-url": "2.12.0",
        "@parcel/packager-raw-url": "2.12.0",
        "@parcel/reporter-bundle-analyzer": "2.12.0",
        "@parcel/transformer-inline-string": "2.12.0",
        "@parcel/transformer-webmanifest": "2.12.0",
        "@tailwindcss/typography": "^0.5.15",
        "@types/eslint__js": "^8.42.3",
        "@types/react": "^18.3.11",
        "@types/react-dom": "^18.3.0",
        "@xpd/tailwind-3dtransforms": "^1.0.3",
        "concurrently": "^8.2.2",
        "cross-env": "^7.0.3",
        "eslint": "^9.12.0",
        "eslint-plugin-only-warn": "^1.1.0",
        "eslint-plugin-react": "^7.37.1",
        "nodemon": "^3.1.7",
        "parcel": "2.12.0",
        "parcel-reporter-static-files-copy": "^1.5.3",
        "parcel-resolver-ignore": "^2.2.0",
        "postcss": "^8.4.47",
        "posthtml": "^0.16.6",
        "posthtml-cli": "^0.10.0",
        "posthtml-doctype": "^1.1.1",
        "posthtml-external-link": "^0.1.2",
        "posthtml-sri": "^1.2.1",
        "process": "^0.11.10",
        "rimraf": "^6.0.1",
        "shadcn": "^2.1.0",
        "tailwindcss": "^3.4.13",
        "tailwindcss-animated": "^1.1.2",
        "tsx": "^4.19.1",
        "typescript": "^5.6.3",
        "typescript-eslint": "8.8.1"
    },
    "dependencies": {
        "@fontsource-variable/inter": "^5.1.0",
        "@hookform/resolvers": "^3.9.0",
        "@radix-ui/react-dialog": "^1.1.2",
        "@radix-ui/react-label": "^2.1.0",
        "@radix-ui/react-select": "^2.1.2",
        "@radix-ui/react-slot": "^1.1.0",
        "better-react-mathjax": "^2.0.3",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.1.1",
        "lucide-react": "^0.451.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-helmet-async": "^2.0.5",
        "react-hook-form": "^7.53.0",
        "react-icons": "^5.3.0",
        "react-router-dom": "6.26.2",
        "tailwind-merge": "^2.5.3",
        "tailwindcss-animate": "^1.0.7",
        "usehooks-ts": "^3.1.0",
        "vaul": "^1.0.0",
        "zod": "^3.23.8"
    }
}
